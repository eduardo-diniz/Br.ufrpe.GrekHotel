<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="610.0" prefWidth="1080.0" styleClass="bodybg" stylesheets="@aplicacao.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufrpe.GrekHotel.GUI.ControladorTelaUser">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="inputLogin" layoutX="483.0" layoutY="289.0" prefHeight="25.0" prefWidth="175.0" promptText="Leandro Nota Boa">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <PasswordField fx:id="inputSenha" layoutX="483.0" layoutY="332.0" prefHeight="25.0" prefWidth="175.0" promptText="**************">
               <font>
                  <Font size="14.0" />
               </font>
            </PasswordField>
            <Button fx:id="btnLogin" layoutX="606.0" layoutY="375.0" mnemonicParsing="false" onAction="#login" prefHeight="36.0" prefWidth="52.0" text="Entrar" />
            <Label layoutX="423.0" layoutY="291.0" prefHeight="25.0" prefWidth="52.0" text="Login:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="423.0" layoutY="335.0" prefHeight="25.0" prefWidth="52.0" text="Senha:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Button fx:id="btnCadastrar" layoutX="483.0" layoutY="375.0" mnemonicParsing="false" onAction="#telaCadastro" prefHeight="36.0" prefWidth="68.0" text="Cadastrar" />
            <Label layoutX="486.0" layoutY="199.0" prefHeight="46.0" prefWidth="105.0" text="Bem vindo">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
